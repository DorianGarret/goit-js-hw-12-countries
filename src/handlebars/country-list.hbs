<ul class='list-group'>
  {{#each this}}
    <li class='list-group-item d-flex align-items-center'><img
        src={{this.flags.svg}}
        class='img-fluid'
        style='max-width: 2rem; margin-right: 10px; border: 1px solid rgba(0,0,0,.125)'
        alt={{name}}
      /><p class='list-text'>{{name}}</p></li>
  {{/each}}
</ul>